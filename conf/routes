GET     /assets/*file               controllers.Assets.at(path="/public", file)

# COURSE
POST     /courses                       @controllers.CourseCtrl.create
GET      /courses                       @controllers.CourseCtrl.getList
GET      /courses/$courseId<[a-z0-9]+>  @controllers.CourseCtrl.getOne(courseId: String)
PUT      /courses/$courseId<[a-z0-9]+>  @controllers.CourseCtrl.edit(courseId: String)
DELETE   /courses/$courseId<[a-z0-9]+>  @controllers.CourseCtrl.remove(courseId: String)

